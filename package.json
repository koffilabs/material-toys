{
  "private": true,
  "name": "material_yue_monorepo",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build-all": "yarn workspaces foreach run build",
    "build-icons": "yarn workspace @material-yue/icons run build",
    "dev": "yarn workspaces foreach --parallel --interlaced run dev",
    "bootstrap": "yarn install"
  },
  "packageManager": "yarn@3.1.0",
  "devDependencies": {
    "@yarnpkg/sdks": "^2.6.0-rc.1"
  }
}
