{
  "private": true,
  "name": "material_yue_monorepo",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "yarn workspaces run build",
    "watchall": "yarn workspaces foreach --parallel --interlaced run dev",
    "bootstrap": "yarn install"
  },
  "packageManager": "yarn@3.1.0"
}
