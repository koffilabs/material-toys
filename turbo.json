{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "emitDeclarations": {},
    "test": {},
    "dev": {
      "persistent": true,
      "cache": false
    },
    "serve": {
      "persistent": true,
      "cache": false
    },
    "build:web": {}
  }
}
